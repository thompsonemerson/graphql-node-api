{
  "name": "graphql-node-api",
  "version": "1.0.0",
  "description": "API Node with GraphQL",
  "main": "dist/index",
  "scripts": {
    "serve": "NODE_ENV=development node_modules/.bin/nodemon --delay 3 .",
    "gulp": "node_modules/.bin/gulp",
    "start": "npm run gulp && node .",
    "dev": "node_modules/.bin/parallelshell 'node_modules/.bin/gulp -- watch' 'npm run serve'"
  },
  "keywords": [
    "api",
    "node",
    "express",
    "graphql"
  ],
  "author": "Emerson Thompson <thompson.emerson@gmail.com> (http://emersonthompson.com.br/)",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "4.0.37",
    "@types/express-graphql": "0.0.34",
    "@types/graphql": "0.11.5",
    "@types/node": "8.0.34",
    "gulp": "^4.0.2",
    "gulp-clean": "0.3.2",
    "gulp-typescript": "^5.0.1",
    "nodemon": "^1.19.1",
    "parallelshell": "^3.0.2",
    "typescript": "2.5.3"
  },
  "dependencies": {
    "express": "4.18.2",
    "express-graphql": "0.6.11",
    "graphql": "0.11.7",
    "graphql-tools": "2.3.0"
  }
}
